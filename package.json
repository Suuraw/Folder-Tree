{
  "name": "generate-folder-tree",
  "displayName": "Generate Folder Tree",
  "description": "Materialize your project's architecture in seconds. This extension understands text-based file trees, allowing you to instantly generate a complete folder structure from sources like LLM suggestions, or manual drawn outlines which depicts folder structure",
  "version": "1.0.1",
  "publisher": "Suuraw",
  "icon": "images/icon.ico",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "folder-tree.generate",
        "title": "Generate Folder Tree: Create from Text Outline"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
